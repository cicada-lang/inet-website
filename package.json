{
  "name": "inet-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "sh prepare.sh && vue-cli-service serve",
    "build": "sh prepare.sh && vue-cli-service build",
    "watch": "tsc --watch",
    "format:ts": "prettier src/**/*.ts --write",
    "format:vue": "prettier src/**/*.vue --write",
    "format": "npm-run-all format:*"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@xieyuheng/ty": "^0.1.13",
    "core-js": "^3.22.7",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-router": "~5.0.4",
    "@vue/cli-plugin-typescript": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "autoprefixer": "^10.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.7.2"
